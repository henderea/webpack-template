{
    "version": 2,
    "name": "webpack-template",
    "public": false,
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "index.html"
        },
        {
            "src": "/(.*)",
            "dest": "$1"
        }
    ]
}